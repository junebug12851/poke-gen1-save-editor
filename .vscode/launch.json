{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main (Debug)",
            "type": "node",
            "request": "launch",
            "env": {
                "DEV": "true"
            },
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "args": [
                "."
            ]
        },
        {
            "name": "Debug Main (Release)",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "args": [
                "."
            ]
        }
    ]
}