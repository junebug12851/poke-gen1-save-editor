                                '[(ngModel)]'='entry.moves[2].moveID'
                            )
                                option([value]='255', disabled='') Choose Move
                                option([value]='0') ----
                                option(
                                    *ngFor='let move of movesList',
                                    '[value]'='move.ind',
                                    '[disabled]'='move.disable'
                                ) {{move.name}}
                        .col.s3
                            label PP
                            input(
                                    type='number'
                                    '[(ngModel)]'='entry.moves[2].pp'
                                )
                        .col.s3
                            label PP UP
                            input(
                                    type='number'
                                    '[(ngModel)]'='entry.moves[2].ppUp'
                                )
                    .row
                        .col.s6
                            label Move 3
                            select.browser-default.black-text(
                                '[(ngModel)]'='entry.moves[3].moveID'
                            )
                                option([value]='255', disabled='') Choose Move
                                option([value]='0') ----
                                option(
                                    *ngFor='let move of movesList',
                                    '[value]'='move.ind',
                                    '[disabled]'='move.disable'
                                ) {{move.name}}
                        .col.s3
                            label PP
                            input(
                                    type='number'
                                    '[(ngModel)]'='entry.moves[3].pp'
                                )
                        .col.s3
                            label PP UP
                            input(
                                    type='number'
                                    '[(ngModel)]'='entry.moves[3].ppUp'
                                )
                #pokemon-0-stats
                    .row
                        .col.s3
