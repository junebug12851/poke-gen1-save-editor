mat-form-field
    mat-label {{label}}
    mat-select(
        '[(ngModel)]'='value'
        '[disabled]'='disabled'
    )
        mat-option('[value]'='0', '[disabled]'='!noneSelectable') Choose Map
        mat-option(
            *ngFor='let map of mapList; trackBy: trackByFn',
            '[value]'='map.value',
            '[disabled]'='map.disable'
        ) {{map.name}}
