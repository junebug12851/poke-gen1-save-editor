mat-form-field
    mat-select(
        '[(ngModel)]'='value'
        '[disabled]'='disabled'
    )
        mat-option('[value]'="0" '[disabled]'='') ---
        mat-option(
            *ngFor='let move of movesList; trackBy: movesTracking',
            '[value]'='move.ind',
            '[disabled]'='move.disable'
        ) {{move.name}}
