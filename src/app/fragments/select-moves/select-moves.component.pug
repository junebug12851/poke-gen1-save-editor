mat-form-field(style="width: 100%")
    mat-select(
        '[(ngModel)]'='value'
        '[disabled]'='disabled'
        required=''
    )
        mat-option('[value]'="255" '[disabled]'='') Choose Move
        mat-option('[value]'="0") ----
        mat-option(
            *ngFor='let move of movesList; trackBy: movesTracking',
            '[value]'='move.ind',
            '[disabled]'='move.disable'
        ) {{move.name}}
